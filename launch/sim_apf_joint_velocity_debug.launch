<?xml version="1.0" ?>
<launch>
  <arg name="robot" default="panda" doc="choose your robot. Possible values: [panda, fr3]"/>
  <arg name="arm_id" default="$(arg robot)" />

  <rosparam command="load" file="$(find fliqc_controller_ros)/config/controller_config.yaml" subst_value="true" />
  
  <include file="$(find robot_env_publisher)/launch/file_env_node.launch"/>

  <include file="$(find fliqc_controller_ros)/launch/gazebo_sim.launch" pass_all_args="true">
    <arg name="controller" value="apf_joint_velocity_debug" />
  </include>
  
  <node name="main_node" pkg="multi_agent_vector_fields" type="test_node"/>

  <node pkg="rviz" type="rviz" output="screen" name="rviz" args="-d $(find fliqc_controller_ros)/launch/sim_apf_joint_velocity_debug.rviz -f $(arg arm_id)_link0 --splash-screen $(find franka_visualization)/splash.png"/>
</launch>